# TCS NextStep Credentials
# Replace with your TCS NextStep login email OR  CT/DT PIN
TCS_EMAIL=your_tcs_email@example.com

# Gmail Credentials for OTP
# The email that will receive the OTP
GMAIL_EMAIL=your_email@gmail.com
# App Password generated from Google Account (16 characters) SEE README.md
GMAIL_APP_PASSWORD=your_16_character_app_password

# Google Gemini API Key
# which you can get from: googlr ai studio
GEMINI_API_KEY=your_gemini_api_key_here




# Application Settings
# Set to False to see the BRrowser (useful for debugging)
HEADLESS=True
# Script timeout in seconds (increase if you have slow internet)
SCRIPT_TIMEOUT=100



